[
    {
      "id": "bot-webhook",
      "execute-command": "sh deploy.sh",
      "command-working-directory": ".",
      "trigger-rule":
      {
        "and":
        [
          {
            "match":
            {
              "type": "payload-hash-sha1",
              "secret": "TxwyAD7ncUvrM",
              "parameter":
              {
                "source": "header",
                "name": "X-Hub-Signature"
              }
            }
          },
          {
            "match":
            {
              "type": "value",
              "value": "refs/heads/master",
              "parameter":
              {
                "source": "payload",
                "name": "ref"
              }
            }
          }
        ]
      }
    }
  ]